<%- include('partials/header')%>
    <div class="card">
        <div class="card-header">
            <div class="head">
                <h1 class="text-center">Updating Data</h1>
            </div>
        </div>
        <div class="card-body">
            <form class="" action="" method="post">
                <div class="row mb-3 bg-body pt-3">
                    <label for="Title" class="col-sm-2 col-form-label txt-edit">Title</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" id="name" placeholder="insert your title" required>
                    </div>
                </div>
                <div class="row mb-3">
                    <label class="col-sm-2 col-form-label">Deadline</label>
                    <div class="col-sm-10">
                        <div class="input-group date" id="datepicker">
                            <input type="date" class="form-control" name="startdate">
                        </div>
                    </div>
                </div>
                <div class="position-relative">
                    <fieldset class="row mb-3">
                        <legend class="col-form-label col-sm-2 pt-0">Complete</legend>
                        <div class="col-sm-10">
                            <div class="form-check">
                                <input class="form-check-input" type="checkbox" id="gridCheck1">
                                <label class="form-check-label" for="gridCheck1">
                                    Done
                                </label>
                            </div>
                    </fieldset>
                </div>
        </div>
        <div class="card-footer">
            <div class="foot-form">
                <div class="formBtm">
                    <button class="btn btn-primary" type="submit">save</button>
                    <a href="/">
                        <button class="btn btn-warning" type="button">Back</button>
                    </a>
                </div>
            </div>
            </form>
        </div>
    </div>
    <%- include('partials/footer')%>